  <URI LOCTYPE="URL" ASSIGNEDBY="local">https://cits3DPM.dilcis.eu/profile/E-ARK-3DPM-ROOT_v1.0.0.xml</URI>
  <title>E-ARK 3D PM root METS Profile 1.0</title>
  <abstract>This is the profile for a root METS following E-ARK 3DPM. It is an extension of the E-ARK CSIP profile.</abstract>
  <date>2024-07-01T12:00:00</date>
  <contact>
    <institution>DILCIS Board</institution>
    <address>http://dilcis.eu/</address>
    <email>info@dilcis.eu</email>
  </contact>
  <related_profile RELATIONSHIP="Extends" URI="https://earksip.dilcis.eu/profile/E-ARK-SIP-v2-2-0.xml">E-ARK SIP METS Profile 2.2.0</related_profile>
  <related_profile RELATIONSHIP="Extends" URI="https://earksip.dilcis.eu/profile/E-ARK-DIP-v2-2-0.xml">E-ARK DIP METS Profile 2.2.0</related_profile>
  <profile_context>
    <resource_model>
      <head>E-ARK 3DPM ROOT profile</head>
      <p xmlns="http://www.w3.org/1999/xhtml">This profile together with the E-ARK CITS 3D PM document describes the root METS document for an IP conforming to the E-ARK CITS 3D PM.</p>
    </resource_model>
  </profile_context>
  <external_schema>
    <name>E-ARK SIP METS Extension</name>
    <URL>https://earksip.dilcis.eu/schema/DILCISExtensionSIPMETS.xsd</URL>
    <context>XML-schema for the attributes added by SIP</context>
    <note>
      <p xmlns="http://www.w3.org/1999/xhtml">An extension schema with the added attributes for use in this profile.</p>
      <p xmlns="http://www.w3.org/1999/xhtml">The schema is used with a namespace prefix of sip.</p>
    </note>
  </external_schema>
  <external_schema>
    <name>E-ARK CSIP METS Extension</name>
    <URL>http://earkcsip.dilcis.eu/schema/DILCISExtensionMETS.xsd</URL>
    <context>XML-schema for the attributes added by CSIP</context>
    <note>
      <p xmlns="http://www.w3.org/1999/xhtml">An extension schema with the added attributes for use in this profile.</p>
      <p xmlns="http://www.w3.org/1999/xhtml">The schema is identified using the namespace prefix csip.</p>
    </note>
  </external_schema>
  <description_rules>
    <p xmlns="http://www.w3.org/1999/xhtml">The filepath must be decoded consistently throughout all file references within the information package.</p>
  </description_rules>
  <controlled_vocabularies>
    <vocabulary ID="VocabularyContentInformationTypeSpecification">
      <name>Content information type specification</name>
      <maintenance_agency>DILCIS Board</maintenance_agency>
      <URI>http://earkcsip.dilcis.eu/schema/CSIPVocabularyContentInformationType.xml</URI>
      <context>Values for `@csip:CONTENTINFORMATIONTYPE`</context>
      <description>
        <p xmlns="http://www.w3.org/1999/xhtml">Lists the names of specific E-ARK content information type specifications supported or maintained in this METS profile.</p>
      </description>
    </vocabulary>
    <vocabulary ID="Vocabulary3DPM">
      <name>Content category and folder definitions in 3DPM</name>
      <maintenance_agency>DILCIS Board</maintenance_agency>
      <URI>https://citsehealth1.dilcis.eu/schema/Vocabulary3DPM.xml</URI>
      <context>Predefined values for content category and documentation types</context>
      <description>
        <p xmlns="http://www.w3.org/1999/xhtml">Describes the extra terms supported by this profile.</p>
        <p xmlns="http://www.w3.org/1999/xhtml">Own names should be placed in an own extending vocabulary.</p>
      </description>
    </vocabulary>
  </controlled_vocabularies>
  <structural_requirements>
    <metsRootElement ID="metsRootElement">
      <requirement ID="3DPM12" REQLEVEL="MUST" EXAMPLES="metsRootElementExample">
        <description>
          <head>METS Profile</head>
          <p>The value is set to "https://cits3dpm.dilcis.eu/profile/E-ARK-3dpm-ROOT.xml".</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/@PROFILE</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM13" REQLEVEL="MUST" RELATEDMAT="VocabularyContentCategory" EXAMPLES="metsRootElementExample">
        <description>
          <head>Content Category</head>
          <p>The `mets/@TYPE` attribute is set to the value "OTHER".</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/@TYPE</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP2</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM14" REQLEVEL="MUST" RELATEDMAT="VocabularyContentCategory Vocabulary3DPM" EXAMPLES="metsRootElementExample">
        <description>
          <head>Other Content Category</head>
          <p>The `mets/@csip:OTHERTYPE` attribute is set to the value "Product Model Data".</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets[@TYPE='OTHER']/@csip:OTHERTYPE</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP3</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM15" REQLEVEL="MUST" RELATEDMAT="VocabularyContentInformationTypeSpecification" EXAMPLES="metsRootElementExample">
        <description>
          <head>Content Information Type Specification</head>
          <p>The `mets/@csip:CONTENTINFORMATIONTYPE` attribute is set to  the value "cits3dpm_v1_0"</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/@csip:CONTENTINFORMATIONTYPE</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP4</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
    </metsRootElement>
    <metsHdr ID="metsHeaderElement">
      <requirement ID="3DPM16" REQLEVEL="SHOULD" RELATEDMAT="VocabularyaltrecordIDTYPE" EXAMPLES="metsRootHdrElementExample">
        <description>
          <head>Submission agreement</head>
          <p>There SHOULD be a reference to the Submission Agreement associated with the package as the SIP will contain personal data.</p>
          <p>`@TYPE` is used with the value "SUBMISSIONAGREEMENT".</p>
          <p>Note: A machine-readable format is recommended for a better description of a submission agreement.</p>
          <p>For example, the submission agreement developed by Docuteam GmbH <a href="http://www.loc.gov/standards/mets/profiles/00000041.xml">http://www.loc.gov/standards/mets/profiles/00000041.xml</a></p>
          <p>A reference code for the Submission Agreement MAY be included with @TYPE used with the value “REFERENCECODE”</p>
          <p>The Submission Agreement SHOULD contain information related to the location in representation descriptive metadata of references to Verification reports.</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>metsHdr/altRecordID[@TYPE='SUBMISSIONAGREEMENT']</dd>
            <dt>Cardinality</dt>
            <dd>0..1</dd>
            <dt>REF:SIP-v2.2.0</dt>
            <dd>https://earksip.dilcis.eu/#SIP5</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
    </metsHdr>
    <dmdSec ID="dmdSecElement">
      <requirement ID="REF_CSIP_1" REQLEVEL="SHOULD" EXAMPLES="dmdSecExample">
        <description>
          <head>Descriptive metadata</head>
          <p>The 3DPM root METS document dmdSec element SHOULD comly with<a xmlns="http://earkcsip.dilcis.eu/#533-use-of-the-mets-descriptive-metadata-section-element-dmdSec">dmdSec requirements in the CSIP profile</a></p>
        </description>
      </requirement>
    </dmdSec>
    <amdSec ID="amdSecElement">
      <requirement ID="REF_CSIP_2" REQLEVEL="SHOULD" EXAMPLES="amdSecExample">
        <description>
          <head>Administrative metadata</head>
          <p>The 3DPM root METS document amdSec element SHOULD comply with <a href="http://earkcsip.dilcis.eu/#534-use-of-the-mets-administrative-metadata-section-element-amdsec"> amdSec requirements in the CSIP profile.</a></p>
        </description>
      </requirement>
    </amdSec>
    <fileSec ID="fileSecElement">
      <requirement ID="3DPM17" REQLEVEL="MUST" EXAMPLES="fileSecExample">
        <description>
          <head>File section</head>
          <p>The transferred content is placed in a representation folder described with a representation METS document.</p>
          <p>Only a single file section (`&lt;fileSec&gt;`) element MUST be present.</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/fileSec</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP58</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM18" REQLEVEL="MUST" EXAMPLES="fileSecExample">
        <description>
          <head>Authentication Documentation file group</head>
          <p>All Authentication Documentation located in the documentation/authentication folder is placed in one or more file group elements with mets/fileSec/fileGrp/@USE attribute value ‘Authentication Documentation’ taken from the vocabulary.</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/fileSec/fileGrp/@ADMID</dd>
            <dt>Cardinality</dt>
            <dd>1..n</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP60</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM19" REQLEVEL="MUST" EXAMPLES="fileSecExample">
        <description>
          <head>Other Documentation file group</head>
          <p>All Other Documentation located in the documentation/other folder is placed in one or more file group elements with mets/fileSec/fileGrp/@USE attribute value ‘Other Documentation’ taken from the vocabulary.</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/fileSec/fileGrp/@ADMID</dd>
            <dt>Cardinality</dt>
            <dd>1..n</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP60</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM20" REQLEVEL="MUST" EXAMPLES="fileSecExample">
        <description>
          <head>Reference to administrative metadata</head>
          <p>If administrative metadata has been provided at file group `mets/fileSec/fileGrp/` level this attribute MUST refer to its administrative metadata section by ID. For example if there are rights and/or digital provenance metadata that are general to the package.</p>
          <p>
            <dt>METS XPath</dt>
            <dd>mets/fileSec/fileGrp/@ADMID</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP61</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
            <dt>dependsOn</dt>
            <dd>CSIP31</dd>
          </p>
        </description>
      </requirement>
      <requirement ID="3DPM21" REQLEVEL="MUST" RELATEDMAT="VocabularyContentInformationTypeSpecification" EXAMPLES="fileSecExample">
        <description>
          <head>Content Information Type Specification</head>
          <p>The value of the attribute mets/fileSec/fileGrp/@csip:CONTENTINFORMATIONTYPE is set to "cits3dpm_v1_0".</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/fileSec/fileGrp[@csip:CONTENTINFORMATIONTYPE='cits3dpm_v1_0']</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>REF:CSIP-v2.2.0</dt>
            <dd>https://earkcsip.dilcis.eu/#CSIP62</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
    </fileSec>
    <structMap ID="structMapElement">
      <requirement ID="3DPM22" REQLEVEL="SHOULD" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation division</head>
          <p>Authentication documentation referenced in the file section is described in the structural map with three sub-divisions</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM23" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation division identifier</head>
          <p>Mandatory, <pre>xml:id</pre> identifier must be unique within the package</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
            <dt>dependsOn</dt>
            <dd>3DPM19</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM24" REQLEVEL="MUST" RELATEDMAT="VocabularyFileGrpAndStructMapDivisionLabel" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation division label</head>
          <p>The Authentication documentation division <pre>&lt;div&gt;</pre> element uses the value Authentication Documentation from the vocabuulary as the value for the @LABEL attribute</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
            <dt>dependsOn</dt>
            <dd>3DPM19</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM25" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation file references</head>
        </description>
        <p>File groups containing Authentication Documentation are referenced via relevant file group identifiers.</p>
        <dl>
          <dt>METS XPath</dt>
          <dd>mets/structMap[@LABEL='CSIP']/div/div/div[@LABEL='Authentication Documentation']/fptr</dd>
          <dt>Cardinality</dt>
          <dd>1..n</dd>
          <dt>since</dt>
          <dd>v1.0.0</dd>
          <dt>dependsOn</dt>
          <dd>3DPM19</dd>
        </dl>
      </requirement>
      <requirement ID="3DPM26" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation file group reference pointer</head>
        </description>
        <p>A reference by ID, to Authentication Documentation file groups,</p>
        <dl>
          <dt>METS XPath</dt>
          <dd>mets/structMap[@LABEL='CSIP']/div/div/div[@LABEL='Authentication Documentation']/fptr/file@ID</dd>
          <dt>Cardinality</dt>
          <dd>1..1</dd>
          <dt>since</dt>
          <dd>v1.0.0</dd>
          <dt>dependsOn</dt>
          <dd>3DPM19</dd>
        </dl>
      </requirement>
      <requirement ID="3DPM27" REQLEVEL="SHOULD" EXAMPLES="structMapExample">
        <description>
          <head>Other Documentation division</head>
          <p>Other documentation referenced in the file section is described in the structural map with three sub-divisions</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM28" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Other Documentation division identifier</head>
          <p>Mandatory, xml:id identifier must be unique within the package</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
            <dt>dependsOn</dt>
            <dd>3DPM24</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM29" REQLEVEL="MUST" RELATEDMAT="VocabularyFileGrpAndStructMapDivisionLabel" EXAMPLES="structMapExample">
        <description>
          <head>Other documentation division label</head>
          <p>The Other documentation division <pre>&lt;div&gt;</pre> element uses the value Other Documentation from the vocabuulary as the value for the @LABEL attribute</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..1</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
            <dt>dependsOn</dt>
            <dd>3DPM24</dd>
          </dl>
        </description>
      </requirement>
      <requirement ID="3DPM30" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation file references</head>
        </description>
        <p>File groups containing Other Documentation are referenced via relevant file group identifiers.</p>
        <dl>
          <dt>METS XPath</dt>
          <dd>mets/structMap[@LABEL='CSIP']/div/div/div[@LABEL='Authentication Documentation']/fptr</dd>
          <dt>Cardinality</dt>
          <dd>1..n</dd>
          <dt>since</dt>
          <dd>v1.0.0</dd>
          <dt>dependsOn</dt>
          <dd>3DPM24</dd>
        </dl>
      </requirement>
      <requirement ID="3DPM31" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Authentication Documentation file group reference pointer</head>
        </description>
        <p>A reference by ID, to Other Documentation file groups,</p>
        <dl>
          <dt>METS XPath</dt>
          <dd>mets/structMap[@LABEL='CSIP']/div/div/div[@LABEL='Authentication Documentation']/fptr/file@ID</dd>
          <dt>Cardinality</dt>
          <dd>1..1</dd>
          <dt>since</dt>
          <dd>v1.0.0</dd>
          <dt>dependsOn</dt>
          <dd>3DPM24</dd>
        </dl>
      </requirement>
      <requirement ID="3DPM32" REQLEVEL="MUST" EXAMPLES="structMapExample">
        <description>
          <head>Representation division</head>
          <p>There must be a discrete div element for each representation of the Product Model.</p>
          <dl>
            <dt>METS XPath</dt>
            <dd>mets/structMap[@LABEL='CSIP']/div/div</dd>
            <dt>Cardinality</dt>
            <dd>1..n</dd>
            <dt>since</dt>
            <dd>v1.0.0</dd>
          </dl>
        </description>
      </requirement>
    </structMap>
